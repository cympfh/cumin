{{
    // OS Environ Values `X` can read with $X or ${X},
    // they are always Strings.
    user: String = ${USER},
    user_alt: String = $USER,  // is also ok

    // Values can be cast to the other types with `as`.
    alpha = ${ALPHA} as Float,

    // Default Value can be set.
    // ${NAME:-DEFAULT}
    beta = ${BETA:-0.3} as Float,
}}
